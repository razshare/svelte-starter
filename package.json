{
  "name": "svelte-mobile-starter",
  "version": "0.1.0",
  "description": "A svelte template for creating mobile applications",
  "type": "module",
  "keywords": [
    "capacitor",
    "mobile"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && npx cap sync",
    "serve": "vite preview",
    "start": "npx cap open android",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write ./src/**/*.{js,svelte,html,ts}",
    "prettier": "prettier --write",
    "lint": "eslint --no-error-on-unmatched-pattern \"./src/**/*.{js,ts,svelte}\"",
    "lint:fix": "eslint --no-error-on-unmatched-pattern --fix \"./src/**/*.{js,ts,svelte}\"",
    "prelint": "npm run format"
  },
  "devDependencies": {
    "@capacitor/android": "^4.4.0",
    "@capacitor/app": "^4.1.0",
    "@capacitor/camera": "latest",
    "@capacitor/cli": "latest",
    "@capacitor/core": "latest",
    "@capacitor/haptics": "^4.0.1",
    "@capacitor/ios": "^4.4.0",
    "@capacitor/keyboard": "^4.0.1",
    "@capacitor/push-notifications": "^4.1.1",
    "@capacitor/splash-screen": "latest",
    "@capacitor/status-bar": "^4.0.1",
    "@mdi/js": "^7.0.96",
    "@sveltejs/vite-plugin-svelte": "*",
    "@tsconfig/svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "autoprefixer": "^10.4.12",
    "daisyui": "^2.33.0",
    "eslint": "^8.26.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.8.0",
    "sass": "^1.55.0",
    "svelte": "^3.52.0",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.2.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.0"
  },
  "author": "",
  "license": "ISC"
}
