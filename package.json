{
  "name": "svelte-mobile-starter",
  "version": "0.1.0",
  "description": "A svelte template for creating mobile applications",
  "type": "module",
  "keywords": [
    "capacitor",
    "mobile"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && npx cap sync",
    "serve": "vite preview",
    "start": "npx cap open android",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write ./src/**/*.{js,svelte,html,ts}",
    "prettier": "prettier --write",
    "lint": "eslint --no-error-on-unmatched-pattern \"./src/**/*.{js,ts,svelte}\"",
    "lint:fix": "eslint --no-error-on-unmatched-pattern --fix \"./src/**/*.{js,ts,svelte}\"",
    "prelint": "npm run format"
  },
  "devDependencies": {
    "@capacitor/android": "^5.0",
    "@capacitor/app": "^5.0",
    "@capacitor/camera": "latest",
    "@capacitor/cli": "latest",
    "@capacitor/core": "latest",
    "@capacitor/haptics": "^5.0",
    "@capacitor/ios": "^5.0",
    "@capacitor/keyboard": "^5.0",
    "@capacitor/push-notifications": "^5.0",
    "@capacitor/splash-screen": "latest",
    "@capacitor/status-bar": "^5.0",
    "@mdi/js": "^7.2",
    "@sveltejs/vite-plugin-svelte": "*",
    "@tsconfig/svelte": "^4.0",
    "@typescript-eslint/eslint-plugin": "^5.59",
    "@typescript-eslint/parser": "^5.59",
    "autoprefixer": "^10.4",
    "daisyui": "^2.51",
    "eslint": "^8.40",
    "eslint-plugin-svelte3": "^4.0",
    "postcss": "^8.4",
    "prettier": "^2.8",
    "prettier-plugin-svelte": "^2.10",
    "sass": "^1.62",
    "svelte": "^3.59",
    "svelte-check": "^3.3",
    "svelte-preprocess": "^5.0",
    "tailwindcss": "^3.3",
    "tslib": "^2.5",
    "typescript": "^5.0",
    "vite": "^4.3"
  },
  "author": "",
  "license": "ISC"
}
