{
  "name": "vitetest",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && bunx cap sync",
    "serve": "vite preview",
    "start": "bunx cap open android",
    "check": "svelte-check",
    "format": "prettier --write ./src/**/*.{js,ts,html,svelte}",
    "prettier": "prettier --write",
    "lint": "eslint --no-error-on-unmatched-pattern \"./src/**/*.{js,ts,html,svelte}\"",
    "lint:fix": "eslint --no-error-on-unmatched-pattern --fix \"./src/**/*.{js,ts,html,svelte}\"",
    "prebuild": "bun run lint:fix",
    "prelint": "bun run format"
  },
  "devDependencies": {
    "@capacitor/android": "5.2.3",
    "@capacitor/app": "5.0.6",
    "@capacitor/camera": "5.0.7",
    "@capacitor/cli": "5.2.3",
    "@capacitor/core": "5.2.3",
    "@capacitor/haptics": "5.0.6",
    "@capacitor/ios": "5.2.3",
    "@capacitor/keyboard": "5.0.6",
    "@capacitor/push-notifications": "5.0.6",
    "@capacitor/splash-screen": "5.0.6",
    "@capacitor/status-bar": "5.0.6",
    "@mdi/js": "7.2.96",
    "@sveltejs/vite-plugin-svelte": "2.4.6",
    "@tsconfig/svelte": "5.0.0",
    "@types/node": "^20.8.7",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "autoprefixer": "10.4.16",
    "bun-types": "latest",
    "daisyui": "3.7.3",
    "eslint": "8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.34.0",
    "postcss": "8.4.28",
    "prettier": "2.8.8",
    "prettier-plugin-svelte": "2.10.1",
    "sass": "1.66.1",
    "svelte": "4.2.0",
    "svelte-check": "3.5.0",
    "svelte-preprocess": "5.0.4",
    "svelte-routing": "2.4.0",
    "tailwindcss": "3.3.3",
    "tslib": "2.6.2",
    "typescript": "5.1.6",
    "vite": "4.4.9"
  }
}
