{
  "name": "vite-svelte-ts-starter",
  "version": "0.0.0",
  "type": "module",
  "lint-staged": {
    "*.{js,ts,svelte}": [
      "svelte-check",
      "npm run lint:fix"
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write ./src/**/*.{js,svelte,html,ts}",
    "prettier": "prettier --write",
    "lint": "eslint --no-error-on-unmatched-pattern \"./src/**/*.{js,ts,svelte}\"",
    "lint:fix": "eslint --no-error-on-unmatched-pattern --fix \"./src/**/*.{js,ts,svelte}\"",
    "prelint": "npm run format"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.8",
    "@tsconfig/svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.50.1",
    "svelte-check": "^2.9.0",
    "svelte-preprocess": "^4.10.7",
    "svelte-routing": "^1.6.0",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3",
    "vite": "^3.1.3"
  }
}
