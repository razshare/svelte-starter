{
	"name": "my-app",
	"version": "1.0.0",
	"description": "My awesome app.",
	"type": "module",
	"keywords": [
		"svelte",
		"capacitor",
		"web",
		"mobile",
		"template"
	],
	"scripts": {
		"dev": "vite",
		"build": "vite build && npx cap sync",
		"serve": "vite preview",
		"start": "npx cap open android",
		"check": "svelte-check",
		"format": "prettier --write ./src/**/*.{js,svelte,html}",
		"prettier": "prettier --write",
		"lint": "eslint --no-error-on-unmatched-pattern \"./src/**/*.{js,svelte}\"",
		"lint:fix": "eslint --no-error-on-unmatched-pattern --fix \"./src/**/*.{js,ts,svelte}\"",
		"prelint": "npm run format"
	},
	"devDependencies": {
		"@capacitor/android": "^5.0",
		"@capacitor/app": "^5.0",
		"@capacitor/camera": "^5.0",
		"@capacitor/cli": "^5.0",
		"@capacitor/core": "^5.0",
		"@capacitor/haptics": "^5.0",
		"@capacitor/ios": "^5.0",
		"@capacitor/keyboard": "^5.0",
		"@capacitor/push-notifications": "^5.0",
		"@capacitor/splash-screen": "^5.0",
		"@capacitor/status-bar": "^5.0",
		"@mdi/js": "^7.2",
		"@sveltejs/vite-plugin-svelte": "^2.4.1",
		"autoprefixer": "^10.4",
		"daisyui": "^2.51",
		"postcss": "^8.4",
		"prettier": "^2.8",
		"prettier-plugin-svelte": "^2.10",
		"sass": "^1.62",
		"svelte": "^3.59",
		"svelte-check": "^3.4",
		"svelte-preprocess": "^5.0",
		"svelte-routing": "^1.8",
		"tailwindcss": "^3.3",
		"vite": "^4.3",
		"eslint": "^8.41",
		"eslint-plugin-svelte3": "^4.0",
		"@typescript-eslint/eslint-plugin": "^5.59",
		"@typescript-eslint/parser": "^5.59"
	},
	"author": "",
	"license": "MIT"
}
