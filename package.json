		"autoprefixer": "^10.4.15",
		"eslint": "^8.47.0",
		"eslint-plugin-svelte": "^2.32.4",
		"postcss": "^8.4.28",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"sass": "^1.66.1",
		"svelte": "^4.2.0",
		"svelte-check": "^3.5.0",
		"svelte-preprocess": "^5.0.4",
		"svelte-routing": "^1.11.0",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.2",
		"typescript": "^5.1.6",
		"vite": "^4.4.9"
