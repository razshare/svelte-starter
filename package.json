{
  "name": "my-application",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && bunx cap sync",
    "serve": "vite preview",
    "start": "bunx cap open android",
    "check": "svelte-check",
    "format": "prettier --write ./src/**/*.{js,ts,html,svelte}",
    "prettier": "prettier --write",
    "lint": "eslint --no-error-on-unmatched-pattern \"./src/**/*.{js,ts,html,svelte}\"",
    "lint:fix": "eslint --no-error-on-unmatched-pattern --fix \"./src/**/*.{js,ts,html,svelte}\"",
    "prebuild": "bun run lint:fix",
    "prelint": "bun run format"
  },
  "devDependencies": {
    "@capacitor/android": "6.0.0",
    "@capacitor/app": "6.0.0",
    "@capacitor/camera": "6.0.0",
    "@capacitor/cli": "6.0.0",
    "@capacitor/core": "6.0.0",
    "@capacitor/haptics": "6.0.0",
    "@capacitor/ios": "6.0.0",
    "@capacitor/keyboard": "6.0.0",
    "@capacitor/push-notifications": "6.0.0",
    "@capacitor/splash-screen": "6.0.0",
    "@capacitor/status-bar": "6.0.0",
    "@mdi/js": "7.4.47",
    "@sveltejs/vite-plugin-svelte": "3.1.0",
    "@tsconfig/svelte": "5.0.4",
    "@types/node": "20.12.12",
    "@typescript-eslint/eslint-plugin": "7.10.0",
    "@typescript-eslint/parser": "7.10.0",
    "autoprefixer": "10.4.19",
    "bun-types": "1.1.10",
    "daisyui": "4.11.1",
    "eslint": "9.3.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-svelte": "2.39.0",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "prettier-plugin-svelte": "3.2.3",
    "sass": "1.77.2",
    "svelte": "4.2.17",
    "svelte-check": "3.7.1",
    "svelte-preprocess": "5.1.4",
    "svelte-routing": "2.13.0",
    "tailwindcss": "3.4.3",
    "tslib": "2.6.2",
    "typescript": "5.4.5",
    "vite": "5.2.14"
  }
}
