{
	"name": "my-app",
	"version": "0.1.0",
	"description": "My awesome app.",
	"type": "module",
	"keywords": [
		"svelte",
		"capacitor",
		"web",
		"mobile",
		"template"
	],
	"scripts": {
		"dev": "vite",
		"build": "vite build && npx cap sync",
		"serve": "vite preview",
		"start": "npx cap open android",
